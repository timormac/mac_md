表结构
USER_ID varchar as user_id
MOBILE_TEL varchar as mobile_tel
BRANCH_NO varchar as branch_no
BUSINESS_FLAG_LAST varchar as business_flag_last
REQUEST_STATUS varchar as request_status
SUBMIT_DATETIME varchar as submit_datetime
CHANNEL_CODE varchar as channel_code
LAST_UPDATE_DATETIME varchar as last_update_detetime
CLIENT_NAME varchar as client_name
ID_NO varchar as id_no
BIRTHDAY varchar as birthday
REQUEST_NO varchar as request_no
